def  sum_of_2d(li):
    sum=0
    even=0
    odd=0
    sumlist=[]
    for i in li:
        sum=0
        for j in range(len(i)):
                       sum=sum+i[j]
        sumlist.append(sum)

    for i in sumlist:
        if i%2==0:
            even=even+1
        else:
            odd=odd+1

    final=[even,odd]
    return final
